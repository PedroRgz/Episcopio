# Episcopio Environment Variables Example
# Copy this file to .env and fill in your values

# PostgreSQL
EP_POSTGRES_USER=episcopio
EP_POSTGRES_PASSWORD=changeme_strong_password
EP_POSTGRES_DATABASE=episcopio

# API Keys (optional for MVP)
EP_APIS_INEGI_TOKEN=your_inegi_token
EP_APIS_TWITTER_BEARER_TOKEN=your_twitter_bearer_token
EP_APIS_NEWSAPI_KEY=your_newsapi_key

# Security
EP_SECURITY_JWT_SECRET=your_jwt_secret_key_change_this
EP_SECURITY_CORS_ALLOWED_ORIGINS=http://localhost:8050,http://localhost:8000
